	public static int[] addone(int[] s,int num){
		int[] k=new int[s.length+1];
		for (int i = 0; i < k.length; i++) {
			if(i<s.length){
				k[i]=s[i];
			}
		}
		k[s.length]=num;
		return k;
	}
	
